# Theme Park Adapters Configuration

# Default provider (disney, seaworld, universal)
THEMEPARK_DEFAULT_PROVIDER=disney

# Disney Configuration (Redeam)
DISNEY_ENABLED=true
DISNEY_API_BASE_URL=https://api.redeam.com/disney
DISNEY_API_KEY=your_disney_api_key_here
DISNEY_API_SECRET=your_disney_api_secret_here
DISNEY_API_TIMEOUT=30
DISNEY_VERIFY_SSL=true

# SeaWorld Configuration (Redeam)
SEAWORLD_ENABLED=true
SEAWORLD_API_BASE_URL=https://api.redeam.com/seaworld
SEAWORLD_API_KEY=your_seaworld_api_key_here
SEAWORLD_API_SECRET=your_seaworld_api_secret_here
SEAWORLD_API_TIMEOUT=30
SEAWORLD_VERIFY_SSL=true

# Universal Studios Configuration (SmartOrder2)
UNIVERSAL_ENABLED=true
UNIVERSAL_API_BASE_URL=https://api.universalstudios.com/smartorder2
UNIVERSAL_API_USERNAME=your_universal_username_here
UNIVERSAL_API_PASSWORD=your_universal_password_here
UNIVERSAL_API_TIMEOUT=30
UNIVERSAL_VERIFY_SSL=true

# Cache Configuration
THEMEPARK_CACHE_ENABLED=true
THEMEPARK_CACHE_TTL=3600

# Logging Configuration
THEMEPARK_LOGGING_ENABLED=false
THEMEPARK_LOG_CHANNEL=stack
